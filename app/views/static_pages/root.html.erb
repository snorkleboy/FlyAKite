<script type="text/javascript">
  <% if current_user() %>
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
    <% else %>
    window.currentUser = null;
  <% end %>

</script>

<main id='root'> static page of id='root' sent from static_pages controller</main>
